@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: 400;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-create:before {
  content: "\e613";
}

.icon-checkbox:before {
  content: "\e637";
}

.icon-directionsbike:before {
  content: "\eb19";
}

.icon-staycurrentportrait:before {
  content: "\ed34";
}

.icon-close:before {
  content: "\e638";
}

.icon-thumbup:before {
  content: "\e64d";
}

.icon-star:before {
  content: "\e733";
}

.icon-edit-copy:before {
  content: "\e62f";
}

.icon-account_box:before {
  content: "\e79d";
}

.icon-account_circle:before {
  content: "\e79e";
}

.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px; /* Preferred icon size */
  display: inline-block;
  width: 1em;
  height: 1em;
  line-height: 1;
  text-transform: none;
  letter-spacing: normal;
  word-wrap: normal;
  white-space: nowrap;
  direction: ltr;
  /* Support for all WebKit browsers. */
  -webkit-font-smoothing: antialiased;
  /* Support for Safari and Chrome. */
  text-rendering: optimizeLegibility;
  /* Support for Firefox. */
  -moz-osx-font-smoothing: grayscale;
  /* Support for IE. */
  font-feature-settings: 'liga';
}

/*---end of common wxss---*/
